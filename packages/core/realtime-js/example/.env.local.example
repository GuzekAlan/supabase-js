# Supabase Configuration
# Copy this file to .env.local and fill in your values

# Your Supabase project URL
NEXT_PUBLIC_SUPABASE_URL=https://your-project.supabase.co

# Your Supabase anon/public key
NEXT_PUBLIC_SUPABASE_KEY=your-anon-key-here

# Your Supabase Realtime URL (usually same as SUPABASE_URL with /realtime/v1 path)
NEXT_PUBLIC_REALTIME_URL=wss://your-project.supabase.co/realtime/v1

# Test user credentials (optional - for quick testing)
NEXT_PUBLIC_TEST_USER_EMAIL=test@example.com
